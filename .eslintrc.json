{
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "extends": [
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "parser": "babel-eslint",
      "project": "./tsconfig.json",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "rules": {
      "no-console": 0,
      "semi": 0,
      "@typescript-eslint/no-explicit-any": 0
    }
  }
  